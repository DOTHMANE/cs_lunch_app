<div class="container p-7 mt-5">
<p style="color: green"><%= notice %></p>

<%= render @employee %>

<div>
  <%= link_to "Edit this employee", edit_employee_path(@employee), class: "btn btn-outline-primary m-2" %> |
  <%= link_to "Back to employees", employees_path, class: "btn btn-outline-primary m-2" %>

  <%= button_to "Destroy this employee", @employee, method: :delete, class: "btn btn-outline-danger m-2" %>
</div>
</div>